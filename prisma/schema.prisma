generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./database.db"
}

model User {
    id              Int        @id @default(autoincrement())
    ip              String     @unique                          
    username        String     @unique                          //input
    firstname       String                                      //input
    lastname        String                                      //input
    email           String                                      //input
    phone           String                                      //input
    visits          String
    admin           Boolean
    create_epoch    DateTime  @default(now())
    last_edit_epoch DateTime  @default(now())
}